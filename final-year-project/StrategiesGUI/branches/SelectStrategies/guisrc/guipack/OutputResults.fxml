<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane minHeight="400.0" minWidth="600.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="guipack.ResultsController">
   <children>
      <TableView fx:id="resultsTable" prefHeight="400.0" prefWidth="241.0">
        <columns>
          <TableColumn fx:id="placeColumn" prefWidth="75.0" text="Place" />
          <TableColumn fx:id="strategyColumn" prefWidth="75.0" text="Strategy" />
            <TableColumn fx:id="pointsColumn" editable="false" prefWidth="75.0" sortType="DESCENDING" text="Points" />
        </columns>
         <columnResizePolicy>
            <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
         </columnResizePolicy>
      </TableView>
   </children>
</AnchorPane>
